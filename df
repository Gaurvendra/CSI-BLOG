#include<bits/stdc++.h>
using namespace std;
long long arr[101][100003];
int main()
{ 

    long long n,W,maxval=0;
    cin >> n >> W;
    long long val[n],wt[n];

    for(long long i=0;i<n;i++)
    {
        cin >> wt[i] >> val[i];
        maxval += val[i];
    }
 
    for(long long i=0;i<=maxval;i++)
        arr[0][i]=1e18;
    for(long long i=0;i<=n;i++)
        arr[i][0]=0;

    for(long long i=1;i<=n;i++)
    {
        for(long long j=1;j<=maxval;j++)
        {
            if(val[i-1]>j)
                arr[i][j] = arr[i-1][j];
            else
                arr[i][j] = min(arr[i-1][j],wt[i-1]+arr[i-1][j-val[i-1]]);
        }
    }
   
    long long ans;
    for(long long i=0;i<=maxval;i++)
    {

        if(arr[n][i]<=W)
            ans = i;
    }
    cout << ans << endl;
    return 0;
}
