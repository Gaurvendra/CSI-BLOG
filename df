
#include<bits/stdc++.h>
using namespace std;
long long arr[101][100003]; // DP matrix
int main()
{

    long long N, W, maxval = 0 ;
    cin >> N >> W;
    long long val[N], wt[N];

    for (long long i = 0; i < N; i++)
    {
        cin >> wt[i] >> val[i];
        maxval += val[i];
    }

    for(long long i = 0; i <= maxval; i++)
        arr[0][i] = 1e18;
    for(long long i = 0; i <= N; i++)
        arr[i][0] = 0;


    for (long long i = 1; i <= N; i++)
    {
        for (long long j = 1; i <= maxval; j++)
        {
            if(val[i - 1] > j)
                arr[i][j] = arr[i - 1][j];
            else
                arr[i][j] = min(arr[i - 1][j], wt[i - 1] + arr[i - 1][j - val[i - 1]]);
        }
    }

    long long ans;
    for(long long i = 0; i <= maxval; i++)
    {

        if(arr[N][i] <= W)
            ans = i;

    }

    cout << ans << endl;

    return 0;
}























